    if ($list[$i] > $list[$j]){
      $temp = $list[$i];
      $list[$i] = $list[$j];
      $list[$j] = $temp;
    }
  }

  if ($list[$i] > $pivot){
    $temp = $list[$i];
    $list[$i] = $list[$rechts];
    $list[$rechts] = $temp;
  } else {
    $i = $rechts;
  }

  return $i;
}

function quicksort(&$list, $links, $rechts){
  if ($links < $rechts){
